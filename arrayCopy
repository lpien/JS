<!DOCTYPE html>
	<html>
		<head><title> arrayCopy </title></head>
		
		<body>
			<script type = "text/javascript">
				//creates new copy of an array
				function copy(x) {
					var array = new Array();
					for (var i = 0; i < x.length; i++){
						array [i] = x[i];
					}
					return array;
				}
				var colors = ["maroon", "red", "orange", "yellow", "olive", "purple", "fuchia", "white", "lime", "green", "navy", "blue", "aqua", "teal", "black", "silver", "gray"];
				var newArr = new Array();
				newArr = copy(colors);
				for (var i = 0; i < newArr.length; i++) {
				document.writeln(newArr[i]+", ");
				}
			</script>
		</body>
	</html>
